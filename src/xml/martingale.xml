
<xml xmlns="https://developers.google.com/blockly/xml" is_dbot="true" collection="false">
  <variables>
    <variable id="initial_stake">initial_stake</variable>
    <variable id="current_stake">current_stake</variable>
    <variable id="martingale_multiplier">martingale_multiplier</variable>
    <variable id="consecutive_losses">consecutive_losses</variable>
    <variable id="last_profit">last_profit</variable>
    <variable id="max_multiplier">max_multiplier</variable>
    <variable id="max_consecutive_losses">max_consecutive_losses</variable>
    <variable id="trade_count">trade_count</variable>
    <variable id="total_profit_loss">total_profit_loss</variable>
    <variable id="recovery_amount">recovery_amount</variable>
  </variables>

  <block type="trade_definition" id="main_trade_definition" deletable="false" x="0" y="60">
    <statement name="TRADE_OPTIONS">
      <block type="trade_definition_market" id="market_block" deletable="false" movable="false">
        <field name="MARKET_LIST">synthetic_index</field>
        <field name="SUBMARKET_LIST">random_index</field>
        <field name="SYMBOL_LIST">1HZ10V</field>
        <next>
          <block type="trade_definition_tradetype" id="tradetype_block" deletable="false" movable="false">
            <field name="TRADETYPECAT_LIST">digits</field>
            <field name="TRADETYPE_LIST">evenodd</field>
            <next>
              <block type="trade_definition_contracttype" id="contracttype_block" deletable="false" movable="false">
                <field name="TYPE_LIST">both</field>
                <next>
                  <block type="trade_definition_candleinterval" id="interval_block" deletable="false" movable="false">
                    <field name="CANDLEINTERVAL_LIST">60</field>
                    <next>
                      <block type="trade_definition_restartbuysell" id="restart_block" deletable="false" movable="false">
                        <field name="TIME_MACHINE_ENABLED">FALSE</field>
                        <next>
                          <block type="trade_definition_restartonerror" id="error_restart_block" deletable="false" movable="false">
                            <field name="RESTARTONERROR">TRUE</field>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
    <statement name="SUBMARKET">
      <block type="trade_definition_tradeoptions" id="trade_options_block">
        <mutation xmlns="http://www.w3.org/1999/xhtml" has_first_barrier="false" has_second_barrier="false" has_prediction="false"></mutation>
        <field name="DURATIONTYPE_LIST">t</field>
        <value name="DURATION">
          <shadow type="math_number" id="duration_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
        <value name="AMOUNT">
          <block type="variables_get">
            <field name="VAR">current_stake</field>
          </block>
        </value>
      </block>
    </statement>
  </block>

  <!-- Enhanced initialization with recovery tracking -->
  <block type="on_start" id="on_start_block" x="0" y="400">
    <statement name="STACK">
      <block type="variables_set">
        <field name="VAR">initial_stake</field>
        <value name="VALUE">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
        <next>
          <block type="variables_set">
            <field name="VAR">martingale_multiplier</field>
            <value name="VALUE">
              <shadow type="math_number">
                <field name="NUM">2</field>
              </shadow>
            </value>
            <next>
              <block type="variables_set">
                <field name="VAR">consecutive_losses</field>
                <value name="VALUE">
                  <shadow type="math_number">
                    <field name="NUM">0</field>
                  </shadow>
                </value>
                <next>
                  <block type="variables_set">
                    <field name="VAR">last_profit</field>
                    <value name="VALUE">
                      <shadow type="math_number">
                        <field name="NUM">0</field>
                      </shadow>
                    </value>
                    <next>
                      <block type="variables_set">
                        <field name="VAR">max_multiplier</field>
                        <value name="VALUE">
                          <shadow type="math_number">
                            <field name="NUM">2</field>
                          </shadow>
                        </value>
                        <next>
                          <block type="variables_set">
                            <field name="VAR">max_consecutive_losses</field>
                            <value name="VALUE">
                              <shadow type="math_number">
                                <field name="NUM">10</field>
                              </shadow>
                            </value>
                            <next>
                              <block type="variables_set">
                                <field name="VAR">trade_count</field>
                                <value name="VALUE">
                                  <shadow type="math_number">
                                    <field name="NUM">0</field>
                                  </shadow>
                                </value>
                                <next>
                                  <block type="variables_set">
                                    <field name="VAR">current_stake</field>
                                    <value name="VALUE">
                                      <block type="variables_get">
                                        <field name="VAR">initial_stake</field>
                                      </block>
                                    </value>
                                    <next>
                                      <block type="variables_set">
                                        <field name="VAR">total_profit_loss</field>
                                        <value name="VALUE">
                                          <shadow type="math_number">
                                            <field name="NUM">0</field>
                                          </shadow>
                                        </value>
                                        <next>
                                          <block type="variables_set">
                                            <field name="VAR">recovery_amount</field>
                                            <value name="VALUE">
                                              <shadow type="math_number">
                                                <field name="NUM">0</field>
                                              </shadow>
                                            </value>
                                            <next>
                                              <block type="text_print">
                                                <value name="TEXT">
                                                  <shadow type="text">
                                                    <field name="TEXT">🚀 ENHANCED MARTINGALE: System initialized with intelligent recovery</field>
                                                  </shadow>
                                                </value>
                                              </block>
                                            </next>
                                          </block>
                                        </next>
                                      </block>
                                    </next>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
  </block>

  <!-- Enhanced before purchase with execution control -->
  <block type="before_purchase" id="before_purchase_block" x="400" y="400">
    <statement name="BEFOREPURCHASE_STACK">
      <block type="text_print">
        <value name="TEXT">
          <block type="text_join">
            <mutation items="6"></mutation>
            <value name="ADD0">
              <shadow type="text">
                <field name="TEXT">📊 TRADE </field>
              </shadow>
            </value>
            <value name="ADD1">
              <block type="math_arithmetic">
                <field name="OP">ADD</field>
                <value name="A">
                  <block type="variables_get">
                    <field name="VAR">trade_count</field>
                  </block>
                </value>
                <value name="B">
                  <shadow type="math_number">
                    <field name="NUM">1</field>
                  </shadow>
                </value>
              </block>
            </value>
            <value name="ADD2">
              <shadow type="text">
                <field name="TEXT">: Stake </field>
              </shadow>
            </value>
            <value name="ADD3">
              <block type="variables_get">
                <field name="VAR">current_stake</field>
              </block>
            </value>
            <value name="ADD4">
              <shadow type="text">
                <field name="TEXT"> USD | Recovery needed: </field>
              </shadow>
            </value>
            <value name="ADD5">
              <block type="text_join">
                <mutation items="2"></mutation>
                <value name="ADD0">
                  <block type="variables_get">
                    <field name="VAR">recovery_amount</field>
                  </block>
                </value>
                <value name="ADD1">
                  <shadow type="text">
                    <field name="TEXT"> USD</field>
                  </shadow>
                </value>
              </block>
            </value>
          </block>
        </value>
        <next>
          <block type="purchase">
            <field name="PURCHASE_LIST">DIGITEVEN</field>
          </block>
        </next>
      </block>
    </statement>
  </block>

  <!-- Enhanced after purchase with intelligent martingale logic -->
  <block type="after_purchase" id="after_purchase_block" x="800" y="400">
    <statement name="AFTERPURCHASE_STACK">
      <block type="text_print">
        <value name="TEXT">
          <shadow type="text">
            <field name="TEXT">⏳ Contract completed, processing enhanced Martingale logic...</field>
          </shadow>
        </value>
        <next>
          <block type="variables_set">
            <field name="VAR">last_profit</field>
            <value name="VALUE">
              <block type="math_arithmetic">
                <field name="OP">MINUS</field>
                <value name="A">
                  <block type="sell_price"></block>
                </value>
                <value name="B">
                  <block type="ask_price"></block>
                </value>
              </block>
            </value>
            <next>
              <block type="variables_set">
                <field name="VAR">trade_count</field>
                <value name="VALUE">
                  <block type="math_arithmetic">
                    <field name="OP">ADD</field>
                    <value name="A">
                      <block type="variables_get">
                        <field name="VAR">trade_count</field>
                      </block>
                    </value>
                    <value name="B">
                      <shadow type="math_number">
                        <field name="NUM">1</field>
                      </shadow>
                    </value>
                  </block>
                </value>
                <next>
                  <block type="variables_set">
                    <field name="VAR">total_profit_loss</field>
                    <value name="VALUE">
                      <block type="math_arithmetic">
                        <field name="OP">ADD</field>
                        <value name="A">
                          <block type="variables_get">
                            <field name="VAR">total_profit_loss</field>
                          </block>
                        </value>
                        <value name="B">
                          <block type="variables_get">
                            <field name="VAR">last_profit</field>
                          </block>
                        </value>
                      </block>
                    </value>
                    <next>
                      <block type="text_print">
                        <value name="TEXT">
                          <block type="text_join">
                            <mutation items="4"></mutation>
                            <value name="ADD0">
                              <shadow type="text">
                                <field name="TEXT">💰 RESULT: P&L: </field>
                              </shadow>
                            </value>
                            <value name="ADD1">
                              <block type="variables_get">
                                <field name="VAR">last_profit</field>
                              </block>
                            </value>
                            <value name="ADD2">
                              <shadow type="text">
                                <field name="TEXT"> USD | Total: </field>
                              </shadow>
                            </value>
                            <value name="ADD3">
                              <block type="text_join">
                                <mutation items="2"></mutation>
                                <value name="ADD0">
                                  <block type="variables_get">
                                    <field name="VAR">total_profit_loss</field>
                                  </block>
                                </value>
                                <value name="ADD1">
                                  <shadow type="text">
                                    <field name="TEXT"> USD</field>
                                  </shadow>
                                </value>
                              </block>
                            </value>
                          </block>
                        </value>
                        <next>
                          <!-- Enhanced martingale logic -->
                          <block type="controls_if">
                            <mutation else="1"></mutation>
                            <value name="IF0">
                              <block type="logic_compare">
                                <field name="OP">LT</field>
                                <value name="A">
                                  <block type="variables_get">
                                    <field name="VAR">last_profit</field>
                                  </block>
                                </value>
                                <value name="B">
                                  <shadow type="math_number">
                                    <field name="NUM">0</field>
                                  </shadow>
                                </value>
                              </block>
                            </value>
                            <statement name="DO0">
                              <!-- On Loss: Enhanced calculation -->
                              <block type="variables_set">
                                <field name="VAR">recovery_amount</field>
                                <value name="VALUE">
                                  <block type="math_arithmetic">
                                    <field name="OP">ADD</field>
                                    <value name="A">
                                      <block type="variables_get">
                                        <field name="VAR">recovery_amount</field>
                                      </block>
                                    </value>
                                    <value name="B">
                                      <block type="math_single">
                                        <field name="OP">ABS</field>
                                        <value name="NUM">
                                          <block type="variables_get">
                                            <field name="VAR">last_profit</field>
                                          </block>
                                        </value>
                                      </block>
                                    </value>
                                  </block>
                                </value>
                                <next>
                                  <block type="controls_if">
                                    <value name="IF0">
                                      <block type="logic_compare">
                                        <field name="OP">LT</field>
                                        <value name="A">
                                          <block type="math_arithmetic">
                                            <field name="OP">ADD</field>
                                            <value name="A">
                                              <block type="variables_get">
                                                <field name="VAR">consecutive_losses</field>
                                              </block>
                                            </value>
                                            <value name="B">
                                              <shadow type="math_number">
                                                <field name="NUM">1</field>
                                              </shadow>
                                            </value>
                                          </block>
                                        </value>
                                        <value name="B">
                                          <block type="variables_get">
                                            <field name="VAR">max_consecutive_losses</field>
                                          </block>
                                        </value>
                                      </block>
                                    </value>
                                    <statement name="DO0">
                                      <!-- Increase consecutive losses -->
                                      <block type="variables_set">
                                        <field name="VAR">consecutive_losses</field>
                                        <value name="VALUE">
                                          <block type="math_arithmetic">
                                            <field name="OP">ADD</field>
                                            <value name="A">
                                              <block type="variables_get">
                                                <field name="VAR">consecutive_losses</field>
                                              </block>
                                            </value>
                                            <value name="B">
                                              <shadow type="math_number">
                                                <field name="NUM">1</field>
                                              </shadow>
                                            </value>
                                          </block>
                                        </value>
                                        <next>
                                          <!-- Enhanced stake calculation -->
                                          <block type="variables_set">
                                            <field name="VAR">current_stake</field>
                                            <value name="VALUE">
                                              <block type="math_arithmetic">
                                                <field name="OP">MULTIPLY</field>
                                                <value name="A">
                                                  <block type="variables_get">
                                                    <field name="VAR">initial_stake</field>
                                                  </block>
                                                </value>
                                                <value name="B">
                                                  <block type="math_single">
                                                    <field name="OP">POW10</field>
                                                    <value name="NUM">
                                                      <block type="math_arithmetic">
                                                        <field name="OP">MULTIPLY</field>
                                                        <value name="A">
                                                          <block type="math_round">
                                                            <field name="OP">ROUND</field>
                                                            <value name="NUM">
                                                              <block type="math_single">
                                                                <field name="OP">LOG10</field>
                                                                <value name="NUM">
                                                                  <block type="variables_get">
                                                                    <field name="VAR">martingale_multiplier</field>
                                                                  </block>
                                                                </value>
                                                              </block>
                                                            </value>
                                                          </block>
                                                        </value>
                                                        <value name="B">
                                                          <block type="variables_get">
                                                            <field name="VAR">consecutive_losses</field>
                                                          </block>
                                                        </value>
                                                      </block>
                                                    </value>
                                                  </block>
                                                </value>
                                              </block>
                                            </value>
                                            <next>
                                              <!-- Ensure minimum stake -->
                                              <block type="controls_if">
                                                <value name="IF0">
                                                  <block type="logic_compare">
                                                    <field name="OP">LT</field>
                                                    <value name="A">
                                                      <block type="variables_get">
                                                        <field name="VAR">current_stake</field>
                                                      </block>
                                                    </value>
                                                    <value name="B">
                                                      <shadow type="math_number">
                                                        <field name="NUM">0.35</field>
                                                      </shadow>
                                                    </value>
                                                  </block>
                                                </value>
                                                <statement name="DO0">
                                                  <block type="variables_set">
                                                    <field name="VAR">current_stake</field>
                                                    <value name="VALUE">
                                                      <shadow type="math_number">
                                                        <field name="NUM">0.35</field>
                                                      </shadow>
                                                    </value>
                                                  </block>
                                                </statement>
                                                <next>
                                                  <block type="text_print">
                                                    <value name="TEXT">
                                                      <block type="text_join">
                                                        <mutation items="4"></mutation>
                                                        <value name="ADD0">
                                                          <shadow type="text">
                                                            <field name="TEXT">🔴 LOSS </field>
                                                          </shadow>
                                                        </value>
                                                        <value name="ADD1">
                                                          <block type="variables_get">
                                                            <field name="VAR">consecutive_losses</field>
                                                          </block>
                                                        </value>
                                                        <value name="ADD2">
                                                          <shadow type="text">
                                                            <field name="TEXT">: Next stake </field>
                                                          </shadow>
                                                        </value>
                                                        <value name="ADD3">
                                                          <block type="text_join">
                                                            <mutation items="2"></mutation>
                                                            <value name="ADD0">
                                                              <block type="variables_get">
                                                                <field name="VAR">current_stake</field>
                                                              </block>
                                                            </value>
                                                            <value name="ADD1">
                                                              <shadow type="text">
                                                                <field name="TEXT"> USD</field>
                                                              </shadow>
                                                            </value>
                                                          </block>
                                                        </value>
                                                      </block>
                                                    </value>
                                                  </block>
                                                </next>
                                              </block>
                                            </next>
                                          </block>
                                        </next>
                                      </block>
                                    </statement>
                                    <next>
                                      <!-- Check if limits exceeded -->
                                      <block type="controls_if">
                                        <value name="IF0">
                                          <block type="logic_compare">
                                            <field name="OP">GTE</field>
                                            <value name="A">
                                              <block type="variables_get">
                                                <field name="VAR">consecutive_losses</field>
                                              </block>
                                            </value>
                                            <value name="B">
                                              <block type="variables_get">
                                                <field name="VAR">max_consecutive_losses</field>
                                              </block>
                                            </value>
                                          </block>
                                        </value>
                                        <statement name="DO0">
                                          <!-- Reset on max losses -->
                                          <block type="variables_set">
                                            <field name="VAR">consecutive_losses</field>
                                            <value name="VALUE">
                                              <shadow type="math_number">
                                                <field name="NUM">0</field>
                                              </shadow>
                                            </value>
                                            <next>
                                              <block type="variables_set">
                                                <field name="VAR">current_stake</field>
                                                <value name="VALUE">
                                                  <block type="variables_get">
                                                    <field name="VAR">initial_stake</field>
                                                  </block>
                                                </value>
                                                <next>
                                                  <block type="variables_set">
                                                    <field name="VAR">recovery_amount</field>
                                                    <value name="VALUE">
                                                      <shadow type="math_number">
                                                        <field name="NUM">0</field>
                                                      </shadow>
                                                    </value>
                                                    <next>
                                                      <block type="text_print">
                                                        <value name="TEXT">
                                                          <shadow type="text">
                                                            <field name="TEXT">⚠️ MAX LIMIT: Reset to base stake - Recovery cleared</field>
                                                          </shadow>
                                                        </value>
                                                      </block>
                                                    </next>
                                                  </block>
                                                </next>
                                              </block>
                                            </next>
                                          </block>
                                        </statement>
                                      </block>
                                    </next>
                                  </block>
                                </next>
                              </block>
                            </statement>
                            <statement name="ELSE">
                              <!-- On Win or Break-even: Enhanced reset -->
                              <block type="controls_if">
                                <value name="IF0">
                                  <block type="logic_compare">
                                    <field name="OP">GT</field>
                                    <value name="A">
                                      <block type="variables_get">
                                        <field name="VAR">last_profit</field>
                                      </block>
                                    </value>
                                    <value name="B">
                                      <shadow type="math_number">
                                        <field name="NUM">0</field>
                                      </shadow>
                                    </value>
                                  </block>
                                </value>
                                <statement name="DO0">
                                  <!-- Win: Full reset with recovery tracking -->
                                  <block type="variables_set">
                                    <field name="VAR">consecutive_losses</field>
                                    <value name="VALUE">
                                      <shadow type="math_number">
                                        <field name="NUM">0</field>
                                      </shadow>
                                    </value>
                                    <next>
                                      <block type="variables_set">
                                        <field name="VAR">current_stake</field>
                                        <value name="VALUE">
                                          <block type="variables_get">
                                            <field name="VAR">initial_stake</field>
                                          </block>
                                        </value>
                                        <next>
                                          <block type="text_print">
                                            <value name="TEXT">
                                              <block type="text_join">
                                                <mutation items="2"></mutation>
                                                <value name="ADD0">
                                                  <shadow type="text">
                                                    <field name="TEXT">🟢 WIN: Enhanced recovery! Recovered </field>
                                                  </shadow>
                                                </value>
                                                <value name="ADD1">
                                                  <block type="text_join">
                                                    <mutation items="2"></mutation>
                                                    <value name="ADD0">
                                                      <block type="variables_get">
                                                        <field name="VAR">recovery_amount</field>
                                                      </block>
                                                    </value>
                                                    <value name="ADD1">
                                                      <shadow type="text">
                                                        <field name="TEXT"> USD + profit</field>
                                                      </shadow>
                                                    </value>
                                                  </block>
                                                </value>
                                              </block>
                                            </value>
                                            <next>
                                              <!-- Reset recovery amount after successful recovery -->
                                              <block type="variables_set">
                                                <field name="VAR">recovery_amount</field>
                                                <value name="VALUE">
                                                  <shadow type="math_number">
                                                    <field name="NUM">0</field>
                                                  </shadow>
                                                </value>
                                              </block>
                                            </next>
                                          </block>
                                        </next>
                                      </block>
                                    </next>
                                  </block>
                                </statement>
                                <next>
                                  <!-- Break-even handling -->
                                  <block type="controls_if">
                                    <value name="IF0">
                                      <block type="logic_compare">
                                        <field name="OP">EQ</field>
                                        <value name="A">
                                          <block type="variables_get">
                                            <field name="VAR">last_profit</field>
                                          </block>
                                        </value>
                                        <value name="B">
                                          <shadow type="math_number">
                                            <field name="NUM">0</field>
                                          </shadow>
                                        </value>
                                      </block>
                                    </value>
                                    <statement name="DO0">
                                      <block type="controls_if">
                                        <value name="IF0">
                                          <block type="logic_compare">
                                            <field name="OP">GT</field>
                                            <value name="A">
                                              <block type="variables_get">
                                                <field name="VAR">consecutive_losses</field>
                                              </block>
                                            </value>
                                            <value name="B">
                                              <shadow type="math_number">
                                                <field name="NUM">0</field>
                                              </shadow>
                                            </value>
                                          </block>
                                        </value>
                                        <statement name="DO0">
                                          <block type="text_print">
                                            <value name="TEXT">
                                              <block type="text_join">
                                                <mutation items="2"></mutation>
                                                <value name="ADD0">
                                                  <shadow type="text">
                                                    <field name="TEXT">🟡 BREAK-EVEN: Maintaining martingale state, recovery needed: </field>
                                                  </shadow>
                                                </value>
                                                <value name="ADD1">
                                                  <block type="text_join">
                                                    <mutation items="2"></mutation>
                                                    <value name="ADD0">
                                                      <block type="variables_get">
                                                        <field name="VAR">recovery_amount</field>
                                                      </block>
                                                    </value>
                                                    <value name="ADD1">
                                                      <shadow type="text">
                                                        <field name="TEXT"> USD</field>
                                                      </shadow>
                                                    </value>
                                                  </block>
                                                </value>
                                              </block>
                                            </value>
                                          </block>
                                        </statement>
                                        <next>
                                          <!-- If no consecutive losses, use base stake -->
                                          <block type="controls_if">
                                            <value name="IF0">
                                              <block type="logic_compare">
                                                <field name="OP">EQ</field>
                                                <value name="A">
                                                  <block type="variables_get">
                                                    <field name="VAR">consecutive_losses</field>
                                                  </block>
                                                </value>
                                                <value name="B">
                                                  <shadow type="math_number">
                                                    <field name="NUM">0</field>
                                                  </shadow>
                                                </value>
                                              </block>
                                            </value>
                                            <statement name="DO0">
                                              <block type="variables_set">
                                                <field name="VAR">current_stake</field>
                                                <value name="VALUE">
                                                  <block type="variables_get">
                                                    <field name="VAR">initial_stake</field>
                                                  </block>
                                                </value>
                                                <next>
                                                  <block type="text_print">
                                                    <value name="TEXT">
                                                      <shadow type="text">
                                                        <field name="TEXT">🟡 BREAK-EVEN: Using base stake</field>
                                                      </shadow>
                                                    </value>
                                                  </block>
                                                </next>
                                              </block>
                                            </statement>
                                          </block>
                                        </next>
                                      </block>
                                    </statement>
                                  </block>
                                </next>
                              </block>
                            </statement>
                            <next>
                              <!-- Enhanced delay with contract completion waiting -->
                              <block type="text_print">
                                <value name="TEXT">
                                  <block type="text_join">
                                    <mutation items="4"></mutation>
                                    <value name="ADD0">
                                      <shadow type="text">
                                        <field name="TEXT">📊 STATS: Total P&L: </field>
                                      </shadow>
                                    </value>
                                    <value name="ADD1">
                                      <block type="variables_get">
                                        <field name="VAR">total_profit_loss</field>
                                      </block>
                                    </value>
                                    <value name="ADD2">
                                      <shadow type="text">
                                        <field name="TEXT"> USD | Consecutive losses: </field>
                                      </shadow>
                                    </value>
                                    <value name="ADD3">
                                      <block type="variables_get">
                                        <field name="VAR">consecutive_losses</field>
                                      </block>
                                    </value>
                                  </block>
                                </value>
                                <next>
                                  <block type="text_print">
                                    <value name="TEXT">
                                      <shadow type="text">
                                        <field name="TEXT">⏳ Waiting 2 seconds before next trade to ensure proper execution...</field>
                                      </shadow>
                                    </value>
                                    <next>
                                      <block type="controls_wait">
                                        <value name="SECONDS">
                                          <shadow type="math_number">
                                            <field name="NUM">2</field>
                                          </shadow>
                                        </value>
                                        <next>
                                          <block type="trade_again"></block>
                                        </next>
                                      </block>
                                    </next>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
  </block>
