<xml xmlns="https://developers.google.com/blockly/xml" is_dbot="true" collection="false">
  <variables>
    <variable id="stake">Stake</variable>
    <variable id="initial_stake">Initial Stake</variable>
    <variable id="prediction">Prediction</variable>
    <variable id="prediction_before_loss">Prediction before loss</variable>
    <variable id="prediction_after_loss">Prediction after loss</variable>
    <variable id="entry_digit">Entrypoint Digit</variable>
    <variable id="expected_profit">Expected Profit</variable>
    <variable id="stop_loss">Stop Loss</variable>
    <variable id="total_lost">Total Lost</variable>
    <variable id="count_loss">Count Loss</variable>
    <variable id="analysis">Analysis</variable>
    <variable id="tick_1">Tick 1</variable>
    <variable id="display_text">Display Text</variable>
  </variables>

  <!-- TRADE DEFINITION -->
  <block type="trade_definition" id="trade_def" deletable="false" x="0" y="60">
    <statement name="TRADE_OPTIONS">
      <block type="trade_definition_market" deletable="false" movable="false">
        <field name="MARKET_LIST">synthetic_index</field>
        <field name="SUBMARKET_LIST">random_index</field>
        <field name="SYMBOL_LIST">1HZ10V</field>
        <next>
          <block type="trade_definition_tradetype" deletable="false" movable="false">
            <field name="TRADETYPECAT_LIST">digits</field>
            <field name="TRADETYPE_LIST">overunder</field>
            <next>
              <block type="trade_definition_contracttype" deletable="false" movable="false">
                <field name="TYPE_LIST">both</field>
                <next>
                  <block type="trade_definition_candleinterval" deletable="false" movable="false">
                    <field name="CANDLEINTERVAL_LIST">60</field>
                    <next>
                      <block type="trade_definition_restartbuysell" deletable="false" movable="false">
                        <field name="TIME_MACHINE_ENABLED">FALSE</field>
                        <next>
                          <block type="trade_definition_restartonerror" deletable="false" movable="false">
                            <field name="RESTARTONERROR">TRUE</field>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>

    <!-- INITIALIZATION -->
    <statement name="INITIALIZATION">
      <block type="variables_set">
        <field name="VAR" id="prediction_before_loss">Prediction before loss</field>
        <value name="VALUE">
          <block type="math_number">
            <field name="NUM">7</field>
          </block>
        </value>
        <next>
          <block type="variables_set">
            <field name="VAR" id="prediction_after_loss">Prediction after loss</field>
            <value name="VALUE">
              <block type="math_number">
                <field name="NUM">7</field>
              </block>
            </value>
            <next>
              <block type="variables_set">
                <field name="VAR" id="entry_digit">Entrypoint Digit</field>
                <value name="VALUE">
                  <block type="math_number">
                    <field name="NUM">9</field>
                  </block>
                </value>
                <next>
                  <block type="variables_set">
                    <field name="VAR" id="stake">Stake</field>
                    <value name="VALUE">
                      <block type="math_number">
                        <field name="NUM">10</field>
                      </block>
                    </value>
                    <next>
                      <block type="variables_set">
                        <field name="VAR" id="initial_stake">Initial Stake</field>
                        <value name="VALUE">
                          <block type="variables_get">
                            <field name="VAR" id="stake">Stake</field>
                          </block>
                        </value>
                        <next>
                          <block type="variables_set">
                            <field name="VAR" id="expected_profit">Expected Profit</field>
                            <value name="VALUE">
                              <block type="math_number">
                                <field name="NUM">50</field>
                              </block>
                            </value>
                            <next>
                              <block type="variables_set">
                                <field name="VAR" id="stop_loss">Stop Loss</field>
                                <value name="VALUE">
                                  <block type="math_number">
                                    <field name="NUM">1000</field>
                                  </block>
                                </value>
                                <next>
                                  <block type="variables_set">
                                    <field name="VAR" id="prediction">Prediction</field>
                                    <value name="VALUE">
                                      <block type="variables_get">
                                        <field name="VAR" id="prediction_before_loss">Prediction before loss</field>
                                      </block>
                                    </value>
                                    <next>
                                      <block type="variables_set">
                                        <field name="VAR" id="analysis">Analysis</field>
                                        <value name="VALUE">
                                          <block type="text">
                                            <field name="TEXT">analysis</field>
                                          </block>
                                        </value>
                                        <next>
                                          <block type="variables_set">
                                            <field name="VAR" id="total_lost">Total Lost</field>
                                            <value name="VALUE">
                                              <block type="math_number">
                                                <field name="NUM">0</field>
                                              </block>
                                            </value>
                                            <next>
                                              <block type="variables_set">
                                                <field name="VAR" id="count_loss">Count Loss</field>
                                                <value name="VALUE">
                                                  <block type="math_number">
                                                    <field name="NUM">0</field>
                                                  </block>
                                                </value>
                                              </block>
                                            </next>
                                          </block>
                                        </next>
                                      </block>
                                    </next>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>

    <!-- TRADE OPTIONS -->
    <statement name="SUBMARKET">
      <block type="trade_definition_tradeoptions">
        <mutation has_first_barrier="false" has_second_barrier="false" has_prediction="true"></mutation>
        <field name="DURATIONTYPE_LIST">t</field>
        <value name="DURATION">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
        </value>
        <value name="AMOUNT">
          <shadow type="math_number">
            <field name="NUM">1</field>
          </shadow>
          <block type="variables_get">
            <field name="VAR" id="stake">Stake</field>
          </block>
        </value>
        <value name="PREDICTION">
          <shadow type="math_number_positive">
            <field name="NUM">1</field>
          </shadow>
          <block type="variables_get">
            <field name="VAR" id="prediction">Prediction</field>
          </block>
        </value>
      </block>
    </statement>
  </block>

  <!-- BEFORE PURCHASE -->
  <block type="before_purchase" id="before_purchase" deletable="false" x="0" y="800">
    <statement name="BEFOREPURCHASE_STACK">
      <block type="controls_if">
        <mutation elseif="1" else="1"></mutation>
        <value name="IF0">
          <block type="logic_compare">
            <field name="OP">EQ</field>
            <value name="A">
              <block type="variables_get">
                <field name="VAR" id="analysis">Analysis</field>
              </block>
            </value>
            <value name="B">
              <block type="text">
                <field name="TEXT">analysis</field>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO0">
          <block type="controls_if">
            <value name="IF0">
              <block type="logic_compare">
                <field name="OP">EQ</field>
                <value name="A">
                  <block type="variables_get">
                    <field name="VAR" id="tick_1">Tick 1</field>
                  </block>
                </value>
                <value name="B">
                  <block type="variables_get">
                    <field name="VAR" id="entry_digit">Entrypoint Digit</field>
                  </block>
                </value>
              </block>
            </value>
            <statement name="DO0">
              <block type="ceo_purchase">
                <field name="PURCHASE_LIST">DIGITUNDER</field>
              </block>
            </statement>
          </block>
        </statement>
        <value name="IF1">
          <block type="logic_compare">
            <field name="OP">EQ</field>
            <value name="A">
              <block type="variables_get">
                <field name="VAR" id="analysis">Analysis</field>
              </block>
            </value>
            <value name="B">
              <block type="text">
                <field name="TEXT">gk</field>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO1">
          <block type="ceo_purchase">
            <field name="PURCHASE_LIST">DIGITUNDER</field>
          </block>
        </statement>
        <statement name="ELSE">
          <block type="controls_if">
            <value name="IF0">
              <block type="logic_compare">
                <field name="OP">EQ</field>
                <value name="A">
                  <block type="variables_get">
                    <field name="VAR" id="analysis">Analysis</field>
                  </block>
                </value>
                <value name="B">
                  <block type="text">
                    <field name="TEXT">Mkorean SV7</field>
                  </block>
                </value>
              </block>
            </value>
            <statement name="DO0">
              <block type="timeout">
                <statement name="TIMEOUTSTACK">
                  <block type="ceo_purchase">
                    <field name="PURCHASE_LIST">DIGITUNDER</field>
                  </block>
                </statement>
                <value name="SECONDS">
                  <block type="math_number">
                    <field name="NUM">0</field>
                  </block>
                </value>
              </block>
            </statement>
          </block>
        </statement>
      </block>
    </statement>
  </block>

  <!-- TICK ANALYSIS -->
  <block type="tick_analysis" id="tick_analysis" x="0" y="1200">
    <statement name="TICKANALYSIS_STACK">
      <block type="variables_set">
        <field name="VAR" id="tick_1">Tick 1</field>
        <value name="VALUE">
          <block type="lists_getIndex">
            <mutation statement="false" at="true"></mutation>
            <field name="MODE">GET</field>
            <field name="WHERE">FROM_END</field>
            <value name="VALUE">
              <block type="lastDigitList"></block>
            </value>
            <value name="AT">
              <block type="math_number">
                <field name="NUM">1</field>
              </block>
            </value>
          </block>
        </value>
        <next>
          <block type="text_join">
            <field name="VARIABLE" id="display_text">Display Text</field>
            <statement name="STACK">
              <block type="text_statement">
                <value name="TEXT">
                  <shadow type="text">
                    <field name="TEXT"></field>
                  </shadow>
                  <block type="text">
                    <field name="TEXT">Last Digit: </field>
                  </block>
                </value>
                <next>
                  <block type="text_statement">
                    <value name="TEXT">
                      <shadow type="text">
                        <field name="TEXT"></field>
                      </shadow>
                      <block type="variables_get">
                        <field name="VAR" id="tick_1">Tick 1</field>
                      </block>
                    </value>
                  </block>
                </next>
              </block>
            </statement>
            <next>
              <block type="notify">
                <field name="NOTIFICATION_TYPE">success</field>
                <field name="NOTIFICATION_SOUND">silent</field>
                <value name="MESSAGE">
                  <shadow type="text">
                    <field name="TEXT">Current Digit</field>
                  </shadow>
                  <block type="variables_get">
                    <field name="VAR" id="tick_1">Tick 1</field>
                  </block>
                </value>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
  </block>

  <!-- AFTER PURCHASE -->
  <block type="after_purchase" id="after_purchase" x="500" y="60">
    <statement name="AFTERPURCHASE_STACK">
      <block type="controls_if">
        <mutation elseif="1" else="1"></mutation>
        <value name="IF0">
          <block type="logic_compare">
            <field name="OP">GT</field>
            <value name="A">
              <block type="total_profit"></block>
            </value>
            <value name="B">
              <block type="variables_get">
                <field name="VAR" id="expected_profit">Expected Profit</field>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO0">
          <block type="text_print">
            <value name="TEXT">
              <shadow type="text">
                <field name="TEXT">CONGRATULATIONS!! Take profit</field>
              </shadow>
            </value>
          </block>
        </statement>
        <value name="IF1">
          <block type="logic_compare">
            <field name="OP">LTE</field>
            <value name="A">
              <block type="total_profit"></block>
            </value>
            <value name="B">
              <block type="math_arithmetic">
                <field name="OP">MULTIPLY</field>
                <value name="A">
                  <shadow type="math_number">
                    <field name="NUM">-1</field>
                  </shadow>
                  <block type="variables_get">
                    <field name="VAR" id="stop_loss">Stop Loss</field>
                  </block>
                </value>
                <value name="B">
                  <shadow type="math_number">
                    <field name="NUM">1</field>
                  </shadow>
                </value>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO1">
          <block type="text_print">
            <value name="TEXT">
              <shadow type="text">
                <field name="TEXT">Stop Loss hit</field>
              </shadow>
            </value>
          </block>
        </statement>
        <statement name="ELSE">
          <block type="controls_if">
            <mutation else="1"></mutation>
            <value name="IF0">
              <block type="contract_check_result">
                <field name="CHECK_RESULT">loss</field>
              </block>
            </value>
            <statement name="DO0">
              <block type="math_change">
                <field name="VAR" id="total_lost">Total Lost</field>
                <value name="DELTA">
                  <shadow type="math_number">
                    <field name="NUM">1</field>
                  </shadow>
                  <block type="variables_get">
                    <field name="VAR" id="stake">Stake</field>
                  </block>
                </value>
                <next>
                  <block type="math_change">
                    <field name="VAR" id="count_loss">Count Loss</field>
                    <value name="DELTA">
                      <shadow type="math_number">
                        <field name="NUM">1</field>
                      </shadow>
                    </value>
                    <next>
                      <block type="controls_if">
                        <value name="IF0">
                          <block type="logic_compare">
                            <field name="OP">EQ</field>
                            <value name="A">
                              <block type="variables_get">
                                <field name="VAR" id="count_loss">Count Loss</field>
                              </block>
                            </value>
                            <value name="B">
                              <block type="math_number">
                                <field name="NUM">1</field>
                              </block>
                            </value>
                          </block>
                        </value>
                        <statement name="DO0">
                          <block type="variables_set">
                            <field name="VAR" id="prediction">Prediction</field>
                            <value name="VALUE">
                              <block type="variables_get">
                                <field name="VAR" id="prediction_after_loss">Prediction after loss</field>
                              </block>
                            </value>
                            <next>
                              <block type="variables_set">
                                <field name="VAR" id="stake">Stake</field>
                                <value name="VALUE">
                                  <block type="math_arithmetic">
                                    <field name="OP">MULTIPLY</field>
                                    <value name="A">
                                      <shadow type="math_number">
                                        <field name="NUM">2</field>
                                      </shadow>
                                      <block type="variables_get">
                                        <field name="VAR" id="stake">Stake</field>
                                      </block>
                                    </value>
                                    <value name="B">
                                      <shadow type="math_number">
                                        <field name="NUM">1</field>
                                      </shadow>
                                    </value>
                                  </block>
                                </value>
                                <next>
                                  <block type="variables_set">
                                    <field name="VAR" id="analysis">Analysis</field>
                                    <value name="VALUE">
                                      <block type="text">
                                        <field name="TEXT">Mkorean SV7</field>
                                      </block>
                                    </value>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </statement>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </statement>
            <statement name="ELSE">
              <block type="variables_set">
                <field name="VAR" id="count_loss">Count Loss</field>
                <value name="VALUE">
                  <block type="math_number">
                    <field name="NUM">0</field>
                  </block>
                </value>
                <next>
                  <block type="variables_set">
                    <field name="VAR" id="stake">Stake</field>
                    <value name="VALUE">
                      <block type="variables_get">
                        <field name="VAR" id="initial_stake">Initial Stake</field>
                      </block>
                    </value>
                    <next>
                      <block type="variables_set">
                        <field name="VAR" id="prediction">Prediction</field>
                        <value name="VALUE">
                          <block type="variables_get">
                            <field name="VAR" id="prediction_before_loss">Prediction before loss</field>
                          </block>
                        </value>
                        <next>
                          <block type="variables_set">
                            <field name="VAR" id="analysis">Analysis</field>
                            <value name="VALUE">
                              <block type="text">
                                <field name="TEXT">gk</field>
                              </block>
                            </value>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </statement>
            <next>
              <block type="trade_again"></block>
            </next>
          </block>
        </statement>
      </block>
    </statement>
  </block>
</xml>